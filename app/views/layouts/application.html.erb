<!DOCTYPE html>
<html>
<head>
  <title>VentasApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div class="header">
      <div class="container">
        <div class="nav">

        <% if current_user %> 
          <ul> 
            <li><%= current_user.store.name %></li> 
            <li><%= link_to "Log out", logout_path, method: "delete" %></li> 
          </ul> 
      	<% end %> 
          
        </div>
      </div>
    </div>

<%= yield %>

</body>
</html>
