<div class="login">
  <div class="container">
    <div class="form">

    <h1>Nuevo Usuario</h1>

    <%= form_for(@user_new) do |f| %>
      <%= f.text_field :name, :placeholder => "Nombre Usuario" %>
      <br>
      <br>
      <%= f.email_field :email, :placeholder => "Email" %>
      <br>
      <br>
      <%= f.password_field :password, :placeholder => "Contrasena" %>
      <br>
      <br>
      <%= f.text_field :cellphone, :placeholder => "Celular" %>
      <br>
      <br>
      <%= f.submit "Crear", class: "btn-submit" %>
    <% end %>

    </div>
  </div>
</div>